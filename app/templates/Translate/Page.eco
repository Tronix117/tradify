<div id="translate-page">
  <h1>Translate Page</h1>

  <table>
    <% for item in @collection.models : %>
      <tr>
        <td><%= item.get 'raw' %></td>
        <td>
          <input type="text" value="" />
          <% for argHelp in item.get 'help': %>
            <a href="#">{<%= argHelp.number %>}</a> <%= argHelp.type %> <%= argHelp.comment %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </table>
</div>